<?xml version="1.0" encoding="UTF-8"?>
<aiml version="2.0">
    <!-- Bot initialization -->

    <category>
        <pattern>HI</pattern>
        <template>
            Hi! I'm Chip, your computer science career advisor. Nice to meet you! I hear you are a student interested in exploring careers in Computer Science. I'm a bot that has been designed to help you along this journey. For me to recommend a career path for you, you may need to answer some questions. If you are ready to start the quiz type 'yes' or type 'no' to exit.
            <think>
                <set name="software">0</set>
                <set name="ux">0</set>
                <set name="data">0</set>
                <set name="security">0</set>
                <set name="database">0</set>
            </think>
        </template>
    </category>

    <category>
        <pattern>YES</pattern>
        <that>IF YOU ARE READY TO START THE QUIZ TYPE YES OR TYPE NO TO EXIT</that>
        <template><srai>QUESTION1</srai></template>
    </category>


    <category>
        <pattern>BYE</pattern>
        <template>Thanks for chatting! Good luck with your career planning. If you would still have questions about your career path or would like to speak to a human, please reach out to your WGU Career Advisor via email at Gabriella@wgu.edu</template>
    </category>

    <category>
        <pattern>EXIT</pattern>
        <template><srai>BYE</srai></template>
    </category>

    <category>
        <pattern>QUIT</pattern>
        <template><srai>BYE</srai></template>
    </category>
    
    <category>
        <pattern>NO</pattern>
        <template><srai>BYE</srai></template>
    </category>
    
    <category>
        <pattern>HEY</pattern>
        <template><srai>HI</srai></template>
    </category>
    
    <category>
        <pattern>HELLO</pattern>
        <template><srai>HI</srai></template>
    </category>
    
     <category>
        <pattern>SUP</pattern>
        <template><srai>HI</srai></template>
    </category>
</aiml>